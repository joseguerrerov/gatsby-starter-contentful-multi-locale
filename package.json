    "@typescript-eslint/parser": "^3.5.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "chalk": "^4.1.0",
    "contentful-import": "^7.9.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
