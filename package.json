    "gatsby-transformer-sharp": "^2.5.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-select": "^3.1.0",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
